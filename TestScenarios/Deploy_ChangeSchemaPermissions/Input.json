            "deny": { "CONTROL": [ "MyUser_Deny" ] }
        }
    }
  },
  "users": {
    "MyUser_Grant": {
        "type": "DatabaseAuthority",
        "loginName": "MyLogin_ChangeSchemaPermissions_Grant"
    },
    "MyUser_Deny": {
        "type": "DatabaseAuthority",
        "loginName": "MyLogin_ChangeSchemaPermissions_Deny"
    },
    "MyUser_Change": {
        "type": "DatabaseAuthority",
        "loginName": "MyLogin_ChangeSchemaPermissions_Change"
    }
  },
  "permissions": {
        "grant": {
            "CONNECT": {
                "dbo": {},
                "MyUser_Change": {},
                "MyUser_Deny": {},
                "MyUser_Grant": {}
            }
        }
  }
}
